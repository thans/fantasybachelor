<div class="glass" ng-click="roundModal.close()"></div>
<div class="modalContent">
    <div class="close" ng-click="roundModal.close()"></div>
    <div class="title">rounds</div>
    <div ng-repeat="round in roundModal.rounds">
        <div class="divider"></div>
        <div ng-click="roundModal.changeRound(round)">
            <span ng-if="roundModal.activeRound === round" class="dotIcon"></span>{{round.name}}
        </div>
    </div>
</div>