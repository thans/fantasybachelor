<div class="slideOutMenu" ng-class="{active: slideOutMenuActive}">
    <div class="slideOutMenuContent">
        <div scrollable>
            <ul>
                <li ng-click="home()"><span class="hoverable">home</span></li>
                <li>
                    <div ng-click="showRounds = !showRounds"><span class="hoverable">rounds</span></div>
                    <ul ng-show="showRounds">
                        <li ng-repeat="round in rounds" ng-click="goToRound(round.id)"><span class="hoverable">{{round.name | lowercase}}</span></li>
                    </ul>
                </li>
                <!--<li>leagues</li>-->
                <li ng-click="leaderboard()"><span class="hoverable">leaderboard</span></li>
                <li ng-click="changeAlias()"><span class="hoverable">screen name</span></li>
                <li ng-click="logout()"><span class="hoverable">log out</span></li>
            </ul>
        </div>
    </div>
    <div class="slideOutMainContent" ng-click="hideSlideOutMenu()">
        <div scrollable>
            <ng-transclude></ng-transclude>
        </div>
    </div>
</div>
